{
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "installCommand": "npm install",
    "framework": "nextjs",
    "functions": {
        "app/api/**/*.ts": {
            "runtime": "nodejs18.x"
        }
    },
    "env": {
        "NEXTAUTH_URL": "@nextauth_url",
        "NEXTAUTH_SECRET": "@nextauth_secret",
        "DATABASE_URL": "@database_url"
    }
}
